    <ImageView
        android:id="@+id/IVFC1"
        android:layout_width="160dp"
        android:layout_height="161dp"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/LFC1Name"
        android:layout_width="161dp"
        android:layout_height="34dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="128dp"
        android:background="#D9484c51"
        android:gravity="left|center"
        android:paddingLeft="8dp"
        android:text="Food Court 1"
        android:textColor="#ffffff"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintLeft_toLeftOf="@+id/IVFC1"
        app:layout_constraintRight_toRightOf="@+id/IVFC1"
        app:layout_constraintTop_toTopOf="@+id/IVFC1" />

    <ImageView
        android:id="@+id/PH1"
        android:layout_width="60dp"
        android:layout_height="44dp"
        android:src="@drawable/user_green"
        app:layout_constraintBottom_toBottomOf="@+id/LFC1Name"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/IVFC1"
        app:layout_constraintTop_toTopOf="@+id/IVFC1"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/IVFC1"
